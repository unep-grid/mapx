{
  "type": "object",
  "required": [
    "date_modified",
    "hostname",
    "side",
    "level",
    "id_log",
    "is_guest",
    "is_static",
    "data"
  ],
  "properties": {
    "date_modified": {
      "type": "integer",
      "title": "Log date"
    },
    "side": {
      "type": "string",
      "title": "Log origin side",
      "default": "",
      "examples": ["browser"],
      "enum": ["browser", "app", "api"]
    },
    "level": {
      "type": "string",
      "title": "Log level",
      "default": "",
      "examples": ["USER_ACTION"],
      "enum": ["ERROR", "WARNING", "MESSAGE", "LOG", "USER_ACTION"]
    },
    "id_log": {
      "type": "string",
      "title": "Log id",
      "default": "",
      "examples": ["view_panel_click"],
      "enum": [
        "session_end",
        "session_start",
        "view_remove",
        "view_add",
        "view_panel_click",
        "project_change",
        "language_change"
      ]
    },
    "is_guest": {
      "type": "boolean",
      "title": "Is guest",
      "default": false,
      "examples": [false]
    },
    "is_static": {
      "type": "boolean",
      "title": "Is static mode",
      "default": false,
      "examples": [false]
    },
    "data": {
      "type": "object",
      "title": "Log additional data"
    },
    "id_user": {
      "type": "number",
      "title": "User id",
      "default": null,
      "examples": [1]
    },
    "id_project": {
      "type": "string",
      "title": "Project id",
      "default": null,
      "examples": ["MX-T6R-PJF-2DF-3OI-LBF"],
      "minLength": 22,
      "maxLength": 22
    },
    "hostname": {
      "type": "string",
      "title": "Integration hostname (RFC 1123)",
      "default": null,
      "examples": ["app.mapx.org"],
      "minLength": 0,
      "maxLength": 253
    }
  }
}
