{
  "name": "@fxi/el",
  "version": "1.0.1",
  "main": "./dist/el.cjs",
  "module": "./dist/el.module.js",
  "unpkg": "./dist/el.umd.js",
  "exports": {
    "require": "./dist/el.cjs",
    "default": "./dist/el.modern.js"
  },
  "amdName": "El",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unep-grid/mapx"
  },
  "description": "Quick DOM elements creation in js.",
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run",
    "test-debug": "node --inspect-brk ./node_modules/.bin/vitest",
    "inspect": "node inspect 127.0.0.1:9229",
    "dev": "microbundle watch -o dist",
    "serve": "live-server example --mount=/dist:dist",
    "build": "microbundle -o dist",
    "prod": "npm run build && npm publish"
  },
  "keywords": [
    "el",
    "dom",
    "html",
    "tags",
    "minimalist",
    "experiment"
  ],
  "author": "unepgrid.ch",
  "license": "MIT",
  "devDependencies": {
    "jsdom": "^22.1.0",
    "microbundle": "^0.14.0",
    "vitest": "^0.34.6"
  },
  "files": [
    "dist",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/unep-grid/mapx"
  },
  "homepage": "https://github.com/unep-grid/mapx/tree/master/app/src/js/el",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@types/dompurify": "^3.2.0",
    "dompurify": "^3.2.4"
  }
}
